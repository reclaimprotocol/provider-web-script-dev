{
  "name": "@reclaimprotocol/provider-web-script-dev",
  "version": "0.1.0",
  "description": "Reclaim InApp Web User Script Development Environment",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "format": "prettier --write .",
    "build:dev": "esbuild --target=chrome58,firefox57,safari11,edge16 src/support/dev_index.ts --bundle --outfile=build/script.js",
